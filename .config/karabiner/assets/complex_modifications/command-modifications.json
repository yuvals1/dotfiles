{
  "title": "Command Modifications",
  "rules": [
    {
      "description": "Command+Comma to Shift+Enter",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "comma",
            "modifiers": {
              "mandatory": ["command"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter",
              "modifiers": ["shift"]
            }
          ]
        }
      ]
    }
  ]
}
