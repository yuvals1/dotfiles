              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "f",
              "modifiers": ["command"]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^net\\.kovidgoyal\\.kitty$",
                "^com\\.apple\\.Terminal$",
                "^com\\.googlecode\\.iterm2$"
              ]
            }
          ]
